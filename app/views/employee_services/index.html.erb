<h1>Listing employee_services</h1>

<table>
  <thead>
    <tr>
      <th>Employee</th>
      <th>Service</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @employee_services.each do |employee_service| %>
      <tr>
        <td><%= employee_service.employee_id %></td>
        <td><%= employee_service.service_id %></td>
        <td><%= link_to 'Show', employee_service %></td>
        <td><%= link_to 'Edit', edit_employee_service_path(employee_service) %></td>
        <td><%= link_to 'Destroy', employee_service, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Employee service', new_employee_service_path %>
